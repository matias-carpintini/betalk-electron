{
  "name": "voltapp",
  "version": "1.0.8",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "DEBUG=electron-forge:* electron-forge make",
    "publish": "DEBUG=electron-forge:* electron-forge publish",
    "dist": "electron-builder"
  },
  "author": "",
  "license": "ISC",
  "description": "volt Electron application",
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron-forge/publisher-github": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5"
  },
  "dependencies": {
    "auto-launch": "^5.0.6",
    "electron-context-menu": "^4.0.4",
    "electron-squirrel-startup": "^1.0.1",
    "minify": "^11.4.1",
    "update-electron-app": "^3.0.0"
  },
  "build": {
    "appId": "com.volt.app",
    "productName": "volt",
    "directories": {
      "output": "releases/${platform}/${arch}"
    },
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./entitlements.plist",
      "entitlementsInherit": "./entitlements.plist",
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "extraResources": [
      "./betalk-extension"
    ],
    "icon": "assets/big icon white rectangle with shadow.png",
    "files": [
      "**/*",
      "!betalk-extension/.git",
      "!**/.git/**"
    ],
    "dmg": {
      "sign": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matias-carpintini/betalk-electron.git"
  },
  "electronVersion": "32.1.2"
}
